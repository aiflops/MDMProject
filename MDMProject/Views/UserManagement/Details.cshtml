@using MDMProject.ViewModels;

@model UserListViewModel
@{
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<div class="row">
    <div class="col-md-12 col-xl-12">
        <div class="card">
            <div class="card-header">
                <h5>Szczegóły</h5>
            </div>
            <div class="card-block">
                @UserDetails()
            </div>
        </div>
    </div>
</div>

@helper UserDetails()
{
    @Html.Partial("_DetailsPartial", Model)
    <p>
        <a href="@(ViewBag.ReturnUrl ?? Url.Action("Index", "Admin"))" class="btn btn-primary">Wróć do listy</a>
    </p>
}